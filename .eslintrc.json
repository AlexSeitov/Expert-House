{
    "env": {
		"browser": true,
		"commonjs": true,
		"es2021": true,
		"jquery": true
    },
    "extends": "airbnb-base/legacy",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
		"linebreak-style": ["error", "windows"],
		"no-console": "warn",
		"no-var": "error",
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"no-unused-vars": "warn",
		"func-names": "off"
    }
}
